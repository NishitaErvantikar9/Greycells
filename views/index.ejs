<div class="container-fluid">
    <div class="container">
        <div class="contact-sec">
            <h2 style="text-align: center; padding-bottom: 30px;">Contact Us</h2>
            <form>
                <div class="row">
                    <div class="col-lg-6 col-md-12 col-sm-12 col">
                        <input type="text" class="form-control" placeholder="Your Name" name="name">
                    </div>
                    <div class="col-lg-6 col-md-12 col-sm-12 col">
                        <input type="text" class="form-control" placeholder="Enter email" name="email">
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-12 col-sm-12 col">
                        <input type="number" class="form-control" placeholder="Phone" name="phone">
                    </div>
                    <div class="col-lg-6 col-md-12 col-sm-12 col">
                        <select class="form-control" id="exampleFormControlSelect1">
                            <option selected>Services*</option>
                            <option>Money Exchange</option>
                            <option>Boosting Business</option>
                        </select>
                    </div>
                </div>
                <div class='row'>
                    <textarea name="message" placeholder="Message*" class="form-control"
                        style="width:97%; height:200px; margin: 0 10px "></textarea>
                </div>
                <button type="submit" class="btn btn-dark mb-2 sub"><span class="submit-btn">SUBMIT NOW</span></button>
            </form>
        </div>

    </div>
